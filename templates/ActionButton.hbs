<div class="action-element ech-blur">
	{{#if hasQuantity}}
	<div class="quantity-1">
		<div class="border-triangle"></div>
		<span>
			{{quantity}}
		</span>
	</div>
	{{/if}}
	{{#if hasQuantitySecondary}}
	<div class="quantity-2">
		<div class="border-triangle"></div>
		<span>

			{{quantitySecondary}}
		</span>
	</div>
	{{/if}}
  <span class="action-element-title">{{{localize label}}}</span>
</div>